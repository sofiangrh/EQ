<?xml version="1.0"?>
<metacity_theme>
<info>
  <name>EQ</name>
  <author>KEIII</author>
  <copyright>GPL</copyright>
  <date>Sep, 2010</date>
  <description>Simple metacity theme</description>
</info>

<frame_geometry name="normal" rounded_top_left="true" rounded_top_right="true" rounded_bottom_left="false" rounded_bottom_right="false">
    <distance name="left_width" value="2"/>
    <distance name="right_width" value="2"/>
    <distance name="bottom_height" value="2"/>
    <distance name="left_titlebar_edge" value="5"/>
    <distance name="right_titlebar_edge" value="5"/>
    <distance name="button_width" value="18"/>
    <distance name="button_height" value="18"/>
    <distance name="title_vertical_pad" value="4"/>
  <border name="title_border" left="0" right="0" top="1" bottom="1"/>
  <border name="button_border" left="1" right="1" top="1" bottom="1"/>
</frame_geometry>

<frame_geometry name="no_borders" parent="normal" rounded_top_left="false" rounded_top_right="false" rounded_bottom_left="false" rounded_bottom_right="false">
  <distance name="left_width" value="0"/>
  <distance name="right_width" value="0"/>
  <distance name="bottom_height" value="0"/>
  <distance name="left_titlebar_edge" value="2"/>
  <distance name="right_titlebar_edge" value="2"/>
  <border name="title_border" left="0" right="0" top="1" bottom="1"/>
  <border name="button_border" left="1" right="1" top="1" bottom="1"/>
</frame_geometry>

<frame_geometry name="utility" parent="normal" title_scale="small" rounded_top_left="false" rounded_top_right="false" rounded_bottom_left="false" rounded_bottom_right="false">
  <distance name="left_width" value="2"/>
  <distance name="right_width" value="2"/>
  <distance name="bottom_height" value="2"/>
  <distance name="left_titlebar_edge" value="4"/>
  <distance name="right_titlebar_edge" value="4"/>
  <distance name="title_vertical_pad" value="4"/>
  <border name="button_border" left="0" right="0" top="0" bottom="0"/>
</frame_geometry>

<frame_geometry name="border" has_title="false">
  <distance name="left_width" value="4"/>
  <distance name="right_width" value="4"/>
  <distance name="bottom_height" value="4"/>
  <distance name="left_titlebar_edge" value="0"/>
  <distance name="right_titlebar_edge" value="0"/>
  <distance name="button_width" value="0"/>
  <distance name="button_height" value="0"/>
  <distance name="title_vertical_pad" value="4"/>
  <border name="title_border" left="0" right="0" top="0" bottom="0"/>
  <border name="button_border" left="0" right="0" top="0" bottom="0"/>
</frame_geometry>

<draw_ops name="bg_blank">
</draw_ops>

<draw_ops name="bg_topbar">
  <gradient type="vertical" x="0" y="2" width="width" height="title_height+4">
    <color value="#474747"/>
    <color value="#404040"/>
  </gradient>
  <line color="#4D4D4D" width="1" x1="0" y1="1" x2="width" y2="1"/>
</draw_ops>

<draw_ops name="bg_topbar_max">
  <gradient type="vertical" x="0" y="1" width="width" height="title_height+4">
    <color value="#474747"/>
    <color value="#404040"/>
  </gradient>
  <line color="#4D4D4D" width="1" x1="0" y1="0" x2="width" y2="0"/>
</draw_ops>

<draw_ops name="bg_topbar_unfocused">
  <gradient type="vertical" x="0" y="2" width="width" height="title_height+4">
    <color value="#595959"/>
    <color value="#616161"/>
  </gradient>
  <line color="#666666" width="1" x1="0" y1="1" x2="width" y2="1"/>
</draw_ops>

<draw_ops name="bg_topbar_max_unfocused">
  <gradient type="vertical" x="0" y="1" width="width" height="title_height+4">
    <color value="#595959"/>
    <color value="#666666"/>
  </gradient>
  <line color="#4D4D4D" width="1" x1="0" y1="0" x2="width" y2="0"/>
</draw_ops>

<draw_ops name="bg_left_title">
  <line color="#474747" width="1" x1="1" y1="1" x2="1" y2="height"/>
  <line color="#333333" width="1" x1="3" y1="0" x2="5" y2="0"/>
  <line color="#333333" width="1" x1="1" y1="2" x2="1" y2="4"/>
  <line color="#333333" width="1" x1="2" y1="1" x2="3" y2="1"/>
  <line color="#4D4D4D" width="1" x1="3" y1="1" x2="5" y2="1"/>
  <line color="#4D4D4D" width="1" x1="1" y1="4" x2="1" y2="5"/>
  <line color="#4D4D4D" width="1" x1="2" y1="2" x2="3" y2="2"/>
</draw_ops>

<draw_ops name="bg_left_title_unfocused">
  <line color="#616161" width="1" x1="1" y1="1" x2="1" y2="height"/>
  <line color="#4D4D4D" width="1" x1="3" y1="0" x2="5" y2="0"/>
  <line color="#4D4D4D" width="1" x1="1" y1="2" x2="1" y2="4"/>
  <line color="#4D4D4D" width="1" x1="2" y1="1" x2="3" y2="1"/>
  <line color="#666666" width="1" x1="3" y1="1" x2="5" y2="1"/>
  <line color="#666666" width="1" x1="1" y1="4" x2="1" y2="5"/>
  <line color="#666666" width="1" x1="2" y1="2" x2="3" y2="2"/>
</draw_ops>

<draw_ops name="bg_left_title_shaded">
<include name="bg_left_title"/>
  <line color="#333333" width="1" x1="3" y1="height-1" x2="5" y2="height-1"/>
  <line color="#333333" width="1" x1="1" y1="height-4" x2="1" y2="height-2"/>
  <line color="#333333" width="1" x1="2" y1="height-2" x2="3" y2="height-2"/>
</draw_ops>

<draw_ops name="bg_right_title">
  <line color="#333333" width="1" x1="width-5" y1="0" x2="width-3" y2="0"/>
  <line color="#333333" width="1" x1="width-2" y1="2" x2="width-2" y2="4"/>
  <line color="#333333" width="1" x1="width-3" y1="1" x2="width-2" y2="1"/>
  <line color="#4D4D4D" width="1" x1="width-5" y1="1" x2="width-3" y2="1"/>
  <line color="#4D4D4D" width="1" x1="width-2" y1="4" x2="width-2" y2="5"/>
  <line color="#4D4D4D" width="1" x1="width-3" y1="2" x2="width-2" y2="2"/>
</draw_ops>

<draw_ops name="bg_right_title_unfocused">
  <line color="#4D4D4D" width="1" x1="width-5" y1="0" x2="width-3" y2="0"/>
  <line color="#4D4D4D" width="1" x1="width-2" y1="2" x2="width-2" y2="4"/>
  <line color="#4D4D4D" width="1" x1="width-3" y1="1" x2="width-2" y2="1"/>
  <line color="#666666" width="1" x1="width-5" y1="1" x2="width-3" y2="1"/>
  <line color="#666666" width="1" x1="width-2" y1="4" x2="width-2" y2="5"/>
  <line color="#666666" width="1" x1="width-3" y1="2" x2="width-2" y2="2"/>
</draw_ops>

<draw_ops name="bg_right_title_shaded">
<include name="bg_right_title"/>
  <line color="#333333" width="1" x1="width-5" y1="height-1" x2="width-3" y2="height-1"/>
  <line color="#333333" width="1" x1="width-2" y1="height-4" x2="width-2" y2="height-2"/>
  <line color="#333333" width="1" x1="width-3" y1="height-2" x2="width-2" y2="height-2"/>
</draw_ops>

<draw_ops name="bg_bottom_title">
  <line color="#2E2E2E" width="1" x1="1" y1="0" x2="width-1" y2="0"/>
</draw_ops>

<draw_ops name="bg_bottom_title_unfocused">
  <line color="#404040" width="1" x1="1" y1="0" x2="width-1" y2="0"/>
</draw_ops>

<draw_ops name="bg_bottom_title_no_borders">
  <line color="#262626" width="1" x1="0" y1="0" x2="width" y2="0"/>
</draw_ops>

<draw_ops name="bg_bottom_title_no_borders_unfocused">
  <line color="#404040" width="1" x1="0" y1="0" x2="width" y2="0"/>
</draw_ops>

<draw_ops name="bg_borders">

</draw_ops>

<draw_ops name="outline">
  <rectangle color="#333333" x="0" y="0" width="width-1" height="height-1"/>
</draw_ops>

<draw_ops name="outline_unfocused">
  <rectangle color="#4D4D4D" x="0" y="0" width="width-1" height="height-1"/>
</draw_ops>

<draw_ops name="botton_outline">
  <line color="#333333" width="1" x1="0" y1="0" x2="width" y2="0"/>
</draw_ops>

<draw_ops name="bg_window_no_borders">
  <include name="bg_topbar"/>
  <include name="outline"/>
</draw_ops>

<draw_ops name="bg_window_borders">
  <include name="bg_window_no_borders"/>
  <include name="bg_borders"/>
</draw_ops>

<draw_ops name="bg_window_borders_unfocused">
  <include name="bg_topbar_unfocused"/>
  <include name="outline_unfocused"/>
</draw_ops>

<constant name="ButtonPad" value="5"/>
<constant name="PrelightPad" value="5"/>

<draw_ops name="bg_button">
  <gradient type="vertical" x="2" y="2" width="width-4" height="height-4">
    <color value="#FFFFFF"/>
    <color value="#BFBFBF"/>
  </gradient>
  <line color="#595959" width="1" x1="3" y1="1" x2="width-3" y2="1"/>
  <line color="#595959" width="1" x1="3" y1="height-2" x2="width-3" y2="height-2"/>
  <line color="#595959" width="1" x1="1" y1="3" x2="1" y2="height-3"/>
  <line color="#595959" width="1" x1="width-2" y1="3" x2="width-2" y2="height-3"/>
  <line color="#595959" width="1" x1="2" y1="2" x2="3" y2="2"/>
  <line color="#595959" width="1" x1="width-3" y1="2" x2="width-2" y2="2"/>
  <line color="#595959" width="1" x1="2" y1="height-3" x2="3" y2="height-3"/>
  <line color="#595959" width="1" x1="width-3" y1="height-3" x2="width-2" y2="height-3"/>
</draw_ops>

<draw_ops name="bg_button_prelight">
</draw_ops>

<draw_ops name="bg_button_pressed">
  <gradient type="vertical" x="2" y="2" width="width-4" height="height-4">
    <color value="#BFBFBF"/>
    <color value="#FFFFFF"/>
  </gradient>
  <line color="#595959" width="1" x1="3" y1="1" x2="width-3" y2="1"/>
  <line color="#595959" width="1" x1="3" y1="height-2" x2="width-3" y2="height-2"/>
  <line color="#595959" width="1" x1="1" y1="3" x2="1" y2="height-3"/>
  <line color="#595959" width="1" x1="width-2" y1="3" x2="width-2" y2="height-3"/>
  <line color="#595959" width="1" x1="2" y1="2" x2="3" y2="2"/>
  <line color="#595959" width="1" x1="width-3" y1="2" x2="width-2" y2="2"/>
  <line color="#595959" width="1" x1="2" y1="height-3" x2="3" y2="height-3"/>
  <line color="#595959" width="1" x1="width-3" y1="height-3" x2="width-2" y2="height-3"/>
</draw_ops>

